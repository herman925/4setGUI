{
  "id": "background",
  "title": "背景資料",
  "entryForm": [
    {
      "id": "student-id",
      "type": "text",
      "label": "學生編號",
      "placeholder": "輸入學生編號",
      "validation": {
        "regex": "^1[0-9]{4}$",
        "errorMessage": "學生編號必須是5位數字，並以1開頭。"
      }
    },
    {
      "id": "school-id",
      "type": "text",
      "label": "學校編號",
      "placeholder": "輸入學校編號",
      "validation": {
        "min": 1,
        "max": 115,
        "errorMessage": "學校編號必須是1到115之間的數字。"
      }
    },
    {
      "id": "school-name",
      "type": "text",
      "label": "學校名稱",
      "placeholder": "輸入學校編號後自動填寫"
    },
    {
      "id": "child-name",
      "type": "text",
      "label": "小朋友姓名",
      "placeholder": "輸入學生編號自動填寫，可自行修改",
      "required": true
    },
    {
      "id": "gender",
      "type": "select",
      "label": "小朋友性別",
      "options": [
        {
          "value": "male",
          "label": "男"
        },
        {
          "value": "female",
          "label": "女"
        }
      ]
    }
  ],
  "questions": []
}